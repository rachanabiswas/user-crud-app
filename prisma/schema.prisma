// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
  runtime  = "nodejs"
}

datasource db {
  provider = "sqlite"
}

model User {
  id        String @id @default(uuid())
  firstName String
  lastName  String
  email     String @unique
  gender    String
  phNumber  String
}
